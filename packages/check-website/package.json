{
  "name": "check-website",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "A fast low dependency checker of html links/references",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/modernweb-dev/rocket.git",
    "directory": "packages/check-website"
  },
  "author": "Modern Web <hello@modern-web.dev> (https://modern-web.dev/)",
  "homepage": "https://rocket.modern-web.dev/docs/tools/check-website/",
  "main": "./index.js",
  "bin": {
    "check-website": "src/cli.js"
  },
  "type": "module",
  "exports": {
    ".": "./src/index.js"
  },
  "scripts": {
    "test": "mocha --timeout 5000 test-node/**/*.test.js test-node/*.test.js",
    "test:watch": "onchange 'src/**/*.js' 'test-node/**/*.js' -- npm test",
    "types:copy": "copyfiles \"./types/**/*.d.ts\" dist-types/"
  },
  "files": [
    "*.js",
    "dist",
    "dist-types",
    "src"
  ],
  "dependencies": {
    "colorette": "^2.0.16",
    "commander": "^9.0.0",
    "mime-types": "^2.1.35",
    "minimatch": "^3.0.4",
    "node-fetch": "^3.0.0",
    "normalize-url": "^7.2.0",
    "sax-wasm": "^2.0.0",
    "slash": "^4.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.0.0"
  },
  "types": "dist-types/index.d.ts"
}
